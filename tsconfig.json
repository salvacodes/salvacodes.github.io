{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "types": ["vitest/globals", "astro/client", "@types/node"],
    "plugins": [
      {
        "name": "@vue/typescript-plugin",
        "location": "node_modules/@vue/language-server",
        "languages": ["vue"]
      }
    ]
  }
}
